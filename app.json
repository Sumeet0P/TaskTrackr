{
  "expo": {
    "name": "TaskTrackr",
    "slug": "TaskTrackr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash_1.jpg",
    "scheme": "tasktrackr",
    "splash": {
      "image": "./assets/images/splash_1.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
      "textColor": "black"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash_1.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash_1.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash_1.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}